{
  "name": "walnut-admin",
  "version": "0.1.0",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "license": "MIT",
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhaocl1997/walnut-admin-client.git"
  },
  "bugs": {
    "url": "https://github.com/Zhaocl1997/walnut-admin-client/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "urls": {
    "doc": "https://walnut-admin-doc.netlify.app",
    "demo": "https://www.walnut-admin.com"
  },
  "scripts": {
    "predev": "esno build/generate/index.ts && npm run icon:dev",
    "dev": "vite",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "prebuild": "esno build/generate/index.ts && npm run icon:prod",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "preview": "vite preview --mode prod",
    "preview:stage": "vite preview --mode staging",
    "release": "standard-version && git push --follow-tags origin naive-ui",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "check:deps:update": "npx npm-check-updates",
    "deploy:stage": "deploy-cli-service deploy --mode staging",
    "deploy:prod": "deploy-cli-service deploy --mode prod",
    "icon:dev": "esno build/icon dev",
    "icon:prod": "esno build/icon prod"
  },
  "dependencies": {
    "@codemirror/autocomplete": "6.9.0",
    "@codemirror/commands": "6.2.5",
    "@codemirror/lang-javascript": "6.2.1",
    "@codemirror/search": "6.5.2",
    "@codemirror/theme-one-dark": "6.1.2",
    "@codemirror/view": "6.17.1",
    "@fingerprintjs/fingerprintjs": "4.0.1",
    "@tinymce/tinymce-vue": "5.1.1",
    "@vueuse/core": "10.4.1",
    "@vueuse/integrations": "10.4.1",
    "ali-oss": "6.18.0",
    "animate.css": "4.1.1",
    "axios": "0.21.1",
    "codemirror": "6.0.1",
    "cropperjs": "2.0.0-beta.2",
    "crypto-js": "4.1.1",
    "easy-fns-ts": "1.2.3",
    "echarts": "5.4.3",
    "highlight.js": "11.8.0",
    "html-to-image": "1.11.11",
    "intro.js": "7.2.0",
    "libphonenumber-js": "1.10.44",
    "lodash-es": "4.17.21",
    "lru-cache": "10.0.1",
    "pinia": "2.1.6",
    "qrcode": "1.5.3",
    "qs": "6.11.2",
    "signature_pad": "4.1.6",
    "socket.io-client": "4.7.2",
    "sortablejs": "1.15.0",
    "tinymce": "6.7.0",
    "untyper": "0.0.7",
    "vue": "3.3.4",
    "vue-codemirror": "6.1.1",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.4",
    "vue-starport": "0.3.0",
    "vue3-mindmap": "0.5.12",
    "web-vitals": "3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.41.0",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@iconify/json": "2.2.111",
    "@iconify/tools": "3.0.5",
    "@iconify/types": "2.0.0",
    "@iconify/utils": "2.1.9",
    "@iconify/vue": "4.1.1",
    "@types/ali-oss": "6.16.8",
    "@types/codemirror": "5.60.10",
    "@types/crypto-js": "4.1.2",
    "@types/intro.js": "5.1.1",
    "@types/lodash-es": "4.17.9",
    "@types/node": "20.5.9",
    "@types/qs": "6.9.8",
    "@types/rollup-plugin-visualizer": "4.2.1",
    "@types/signature_pad": "2.3.2",
    "@types/sortablejs": "1.15.2",
    "@unocss/preset-icons": "0.55.7",
    "@unocss/reset": "0.55.7",
    "@unocss/transformer-variant-group": "0.55.7",
    "@vitejs/plugin-legacy": "4.1.1",
    "@vitejs/plugin-vue": "4.3.4",
