{
  "name": "walnut-admin",
  "type": "module",
  "version": "0.1.0",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "license": "MIT",
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhaocl1997/walnut-admin-client.git"
  },
  "bugs": {
    "url": "https://github.com/Zhaocl1997/walnut-admin-client/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "urls": {
    "doc": "https://walnut-admin-doc.netlify.app",
    "demo": "https://www.walnut-admin.com"
  },
  "scripts": {
    "predev": "esno build/generate/index.ts && npm run icon:dev",
    "dev": "vite",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "prebuild": "esno build/generate/index.ts && npm run icon:prod",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "preview": "vite preview --mode prod",
    "preview:stage": "vite preview --mode staging",
    "release": "standard-version && git push --follow-tags origin naive-ui",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "check:deps:update": "npx npm-check-updates -i",
    "check:nm": "node-modules-inspector",
    "deploy:stage": "deploy-cli-service deploy --mode staging",
    "deploy:prod": "deploy-cli-service deploy --mode prod",
    "icon:dev": "esno build/icon dev",
    "icon:prod": "esno build/icon prod"
  },
  "dependencies": {
    "@codemirror/autocomplete": "6.18.6",
    "@codemirror/commands": "6.8.1",
    "@codemirror/lang-javascript": "6.2.3",
    "@codemirror/search": "6.5.10",
    "@codemirror/theme-one-dark": "6.1.2",
    "@codemirror/view": "6.36.5",
    "@fingerprintjs/fingerprintjs": "4.6.2",
    "@tinymce/tinymce-vue": "6.1.0",
    "@vueuse/core": "13.1.0",
    "@vueuse/integrations": "13.1.0",
    "ali-oss": "6.22.0",
    "animate.css": "4.1.1",
    "axios": "1.8.4",
    "codemirror": "6.0.1",
    "cropperjs": "2.0.0",
    "crypto-js": "4.2.0",
    "easy-fns-ts": "1.2.3",
    "echarts": "5.6.0",
    "highlight.js": "11.11.1",
    "html-to-image": "1.11.13",
    "intro.js": "7.2.0",
    "libphonenumber-js": "1.12.7",
    "lodash-es": "4.17.21",
    "lru-cache": "11.1.0",
    "nanoid": "5.1.5",
    "pinia": "3.0.2",
    "qs": "6.14.0",
    "signature_pad": "5.0.7",
    "socket.io-client": "4.8.1",
    "sortablejs": "1.15.6",
    "tinymce": "7.8.0",
    "ua-parser-js": "2.0.3",
    "untyper": "0.0.10",
    "vue": "3.5.13",
    "vue-codemirror": "6.1.1",
    "vue-i18n": "11.1.3",
    "vue-router": "4.5.0",
    "vue-starport": "0.4.0",
    "vue3-mindmap": "0.5.12",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@iconify/json": "2.2.329",
    "@iconify/tools": "4.1.2",
    "@iconify/types": "2.0.0",
    "@iconify/utils": "2.3.0",
    "@iconify/vue": "4.3.0",
    "@types/ali-oss": "6.16.11",
    "@types/codemirror": "5.60.15",
    "@types/crypto-js": "4.2.2",
    "@types/intro.js": "5.1.5",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.14.1",
    "@types/qs": "6.9.18",
    "@types/rollup-plugin-visualizer": "5.0.3",
    "@types/signature_pad": "4.0.0",
    "@types/sortablejs": "1.15.8",
    "@unocss/eslint-config": "66.0.0",
    "@unocss/eslint-plugin": "66.0.0",
    "@unocss/preset-icons": "66.0.0",
    "@unocss/reset": "66.0.0",
    "@unocss/transformer-variant-group": "66.0.0",
    "@vitejs/plugin-legacy": "6.1.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitejs/plugin-vue-jsx": "4.1.2",
    "@vue/compiler-sfc": "3.5.13",
    "case-police": "2.0.0",
    "chalk": "5.4.1",
    "commitizen": "4.3.1",
    "conventional-changelog-cli": "5.0.0",
    "cz-custom": "0.0.2",
    "deploy-cli-service": "1.5.3",
    "eslint": "9.25.0",
    "eslint-plugin-vue": "10.0.0",
    "esno": "4.8.0",
    "fast-glob": "3.3.3",
    "is-ci": "4.1.0",
    "javascript-obfuscator": "4.1.1",
    "lint-staged": "15.5.1",
    "madge": "8.0.0",
    "mockjs": "1.1.0",
    "naive-ui": "2.41.0",
    "node-modules-inspector": "0.6.6",
    "npm-check-updates": "17.1.18",
    "rimraf": "6.0.1",
    "rollup": "4.40.0",
    "rollup-plugin-obfuscator": "1.1.0",
    "rollup-plugin-visualizer": "5.14.0",
    "sass": "1.86.3",
    "sharp": "0.34.1",
    "simple-git-hooks": "2.12.1",
    "standard-version": "9.5.0",
    "terser": "5.39.0",
    "ts-json-schema-generator": "2.4.0",
    "typescript": "5.8.3",
    "unocss": "66.0.0",
    "unplugin-auto-import": "19.1.2",
    "unplugin-turbo-console": "2.1.3",
    "unplugin-vue-components": "28.5.0",
    "vfonts": "0.1.0",
    "vite": "6.3.2",
    "vite-plugin-banner": "0.8.1",
    "vite-plugin-cdn2": "1.1.0",
    "vite-plugin-checker": "0.9.3",
    "vite-plugin-compression2": "1.3.3",
    "vite-plugin-image-optimizer": "1.1.8",
    "vite-plugin-mkcert": "1.17.8",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-restart": "0.4.2",
    "vite-plugin-vue-devtools": "7.7.5",
    "vue-tsc": "2.2.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-custom"
    },
    "cz-custom": {
      "config": "config/cz-custom/.cz-config.js"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
