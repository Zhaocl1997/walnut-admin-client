{
  "name": "walnut-admin",
  "type": "module",
  "version": "0.1.0",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "license": "MIT",
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhaocl1997/walnut-admin-client.git"
  },
  "bugs": {
    "url": "https://github.com/Zhaocl1997/walnut-admin-client/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "urls": {
    "doc": "https://walnut-admin-doc.netlify.app",
    "demo": "https://www.walnut-admin.com"
  },
  "scripts": {
    "predev": "esno build/generate/index.ts && npm run icon:dev",
    "dev": "vite",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "prebuild": "esno build/generate/index.ts && npm run icon:prod",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "preview": "vite preview --mode prod",
    "preview:stage": "vite preview --mode staging",
    "release": "standard-version && git push --follow-tags origin naive-ui",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "check:deps:update": "npx npm-check-updates",
    "deploy:stage": "deploy-cli-service deploy --mode staging",
    "deploy:prod": "deploy-cli-service deploy --mode prod",
    "icon:dev": "esno build/icon dev",
    "icon:prod": "esno build/icon prod"
  },
  "dependencies": {
    "@codemirror/autocomplete": "6.18.0",
    "@codemirror/commands": "6.6.0",
    "@codemirror/lang-javascript": "6.2.2",
    "@codemirror/search": "6.5.6",
    "@codemirror/theme-one-dark": "6.1.2",
    "@codemirror/view": "6.30.0",
    "@fingerprintjs/fingerprintjs": "4.4.3",
    "@tinymce/tinymce-vue": "6.0.1",
    "@vueuse/core": "10.11.0",
    "@vueuse/integrations": "10.11.0",
    "ali-oss": "6.20.0",
    "animate.css": "4.1.1",
    "axios": "1.7.7",
    "codemirror": "6.0.1",
    "cropperjs": "2.0.0-beta.2",
    "crypto-js": "4.2.0",
    "easy-fns-ts": "1.2.3",
    "echarts": "5.5.1",
    "highlight.js": "11.10.0",
    "html-to-image": "1.11.11",
    "intro.js": "7.2.0",
    "libphonenumber-js": "1.11.5",
    "lodash-es": "4.17.21",
    "lru-cache": "11.0.0",
    "nanoid": "5.0.7",
    "pinia": "2.2.1",
    "qrcode": "1.5.4",
    "qs": "6.13.0",
    "signature_pad": "5.0.2",
    "socket.io-client": "4.7.5",
    "sortablejs": "1.15.2",
    "tinymce": "7.3.0",
    "untyper": "0.0.10",
    "vue": "3.4.36",
    "vue-codemirror": "6.1.1",
    "vue-i18n": "9.13.1",
    "vue-router": "4.4.3",
    "vue-starport": "0.4.0",
    "vue3-mindmap": "0.5.12",
    "web-vitals": "4.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.24.1",
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@iconify/json": "2.2.234",
    "@iconify/tools": "4.0.4",
    "@iconify/types": "2.0.0",
    "@iconify/utils": "2.1.30",
    "@iconify/vue": "4.1.2",
    "@types/ali-oss": "6.16.11",
    "@types/codemirror": "5.60.15",
    "@types/crypto-js": "4.2.2",
    "@types/intro.js": "5.1.5",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.1.0",
    "@types/qs": "6.9.15",
    "@types/rollup-plugin-visualizer": "4.2.4",
    "@types/signature_pad": "2.3.6",
    "@types/sortablejs": "1.15.8",
    "@unocss/eslint-plugin": "0.61.9",
    "@unocss/preset-icons": "0.61.9",
    "@unocss/reset": "0.61.9",
    "@unocss/transformer-variant-group": "0.61.9",
    "@vitejs/plugin-legacy": "5.4.1",
    "@vitejs/plugin-vue": "5.1.2",
    "@vitejs/plugin-vue-jsx": "4.0.0",
    "@vue/compiler-sfc": "3.4.36",
    "case-police": "0.6.1",
    "chalk": "5.3.0",
    "commitizen": "4.3.0",
    "conventional-changelog-cli": "5.0.0",
    "cz-custom": "0.0.2",
    "deploy-cli-service": "1.5.3",
    "eslint": "9.8.0",
    "eslint-plugin-vue": "9.27.0",
    "esno": "4.7.0",
    "fast-glob": "3.3.2",
    "is-ci": "3.0.1",
    "javascript-obfuscator": "4.1.1",
    "lint-staged": "15.2.8",
    "madge": "8.0.0",
    "mockjs": "1.1.0",
    "naive-ui": "2.39.0",
    "npm-check-updates": "17.0.3",
    "rimraf": "6.0.1",
    "rollup": "4.20.0",
    "rollup-plugin-obfuscator": "1.1.0",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.77.8",
    "sharp": "0.33.4",
    "simple-git-hooks": "2.11.1",
    "standard-version": "9.5.0",
    "terser": "5.31.3",
    "typescript": "5.5.4",
    "typescript-json-schema": "0.64.0",
    "unocss": "0.61.9",
    "unplugin-auto-import": "0.18.2",
    "unplugin-vue-components": "0.27.3",
    "vfonts": "0.1.0",
    "vite": "5.3.5",
    "vite-plugin-banner": "0.7.1",
    "vite-plugin-cdn2": "1.1.0",
    "vite-plugin-compression2": "1.1.3",
    "vite-plugin-image-optimizer": "1.1.8",
    "vite-plugin-inspect": "0.8.5",
    "vite-plugin-mkcert": "1.17.5",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-restart": "0.4.1",
    "vite-plugin-terminal": "1.2.0",
    "vue-tsc": "2.0.29"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-custom"
    },
    "cz-custom": {
      "config": "config/cz-custom/.cz-config.js"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
